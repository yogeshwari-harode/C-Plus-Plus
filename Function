#include <iostream> 
using namespace std; 

int maxi(int x, int y) 
{ 
	if (x > y) 
	return x; 
	else
	return y; 
} 

int main() { 
	int a,b; 
	cout<<"Enter the value of a";
	cin>>a;
	cout<<"Enter the value of b";
	cin>>b;
	// Calling above function to find max of 'a' and 'b' 
	int m=maxi(a,b); 

	cout << "maximum number is "<< m; 
	return 0; 
} 

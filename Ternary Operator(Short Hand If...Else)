#include <iostream>
#include <string>
using namespace std;

int main() {
  int a,b;
  cout<<"Enter the value of a = ";
  cin>>a;
  cout<<"Enter the value of b = ";
  cin>>b;
  string result= (a > b) ? "a is greater" : "b is greater";
  cout << result;
  return 0;
}

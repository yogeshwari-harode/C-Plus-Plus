#include<bits/stdc++.h>
using namespace std;

class A
{    
	private:    
		int a;   
	public:    
		A(int k)    //A constructor    
		{       
		a=k;        
		cout<<"a = "<<a<<endl;    
		}
};


class B : public A
{    
	int b;  
	public:    
		B(int x,int y):A(x) //B constructor    
		{        
			b=y;        
			cout<<"b = "<<b;   
		}
};


int main()
{    
	B obj(2,3);    
	return 0;
}

#include<bits/stdc++.h>
using namespace std;
void swap();

int main()
{
	swap();
	return 0;
}

void swap()
{
	
	int a,b;
	cout<<"Enter the value of a : ";
	cin>>a;
	cout<<"Enter the value of b : ";
	cin>>b;
	a=a-b;
	b=a+b;
	a=b-a;
	
	cout<<"\nThe value of a is : "<<a<<"\n";
	cout<<  "The value of b is : "<<b;
	
}
